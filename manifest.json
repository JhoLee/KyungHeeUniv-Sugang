{
	"manifest_version": 2,
	"name": "Kyung Hee University Sugang Macro",
	"version": "0.1",
	"description": "Hello World!",
	"permissions": [
		"activeTab",
		"tabs",
		"http://sugang.khu.ac.kr/",
		"https://*/"
	],
    "browser_action": {
    	"default_icon": "images/icon.png"
    },
    "icons": {
    	"16": "images/favicon.ico"
    },
	"background": {
		"scripts": ["script.js"],
		"persistent": true
	}

}
