{
	"manifest_version": 2,
	"name": "Kyung Hee University Sugang Macro",
	"version": "0.1",
	"description": "Hello World!",
	"permissions": [
		"activeTab", 
		"webNavigation",
		"http://sugang.khu.ac.kr/*"
	],
    "browser_action": {
    	"default_icon": "images/icon.png",
    	"default_popup": "popup.html"
    },
    "icons": {
    	"16": "images/favicon.ico"
    },
    "content_scripts": [{
    		"matches": ["http://sugang.khu.ac.kr/"],
    		"run_at": "document_end",
    		"js": ["exploit.js"],
    		"all_frames": true
    }]
}
